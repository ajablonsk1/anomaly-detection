{
  "name": "Regularized",
  "description": "Parameters with stronger regularization to prevent overfitting",
  "preprocessing": {
    "variance_threshold": 0.01,
    "correlation_threshold": 0.90,
    "test_size": 0.3,
    "random_state": 42
  },
  "models": {
    "rf": {
      "n_estimators": 150,
      "max_depth": 15,
      "min_samples_split": 10,
      "min_samples_leaf": 5,
      "max_features": "sqrt",
      "random_state": 42,
      "n_jobs": -1
    },
    "xgb": {
      "n_estimators": 150,
      "max_depth": 8,
      "learning_rate": 0.05,
      "subsample": 0.7,
      "colsample_bytree": 0.7,
      "gamma": 1,
      "reg_alpha": 0.1,
      "reg_lambda": 1.0,
      "random_state": 42
    },
    "svm": {
      "C": 0.5,
      "kernel": "rbf",
      "gamma": "scale",
      "random_state": 42
    }
  }
}